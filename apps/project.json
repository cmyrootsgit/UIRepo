{
    "name": "@cmyroots/apps",
    "$schema": "../node_modules/nx/schemas/project-schema.json",
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "vite build",
                "cwd": "apps"
            },
            "dependsOn": [
                "^build"
            ],
            "inputs": [
                "production",
                "^production"
            ],
            "outputs": [
                "{projectRoot}/dist"
            ]
        }
    }
}